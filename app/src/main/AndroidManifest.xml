<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sazzadsblog">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".one" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".two" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".three" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".four" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".five" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".six" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".seven" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".eight" android:parentActivityName=".MainActivity"></activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>